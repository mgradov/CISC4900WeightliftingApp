<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.CISC4900.WeightliftingApp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">
        <activity android:name="com.example.CISC4900.WeightliftingApp.ChartActivity">

        </activity>
        <activity android:name="com.example.CISC4900.WeightliftingApp.SearchActivity">

        </activity>
        <activity android:name="com.example.CISC4900.WeightliftingApp.SettingsFragment" />
        <activity android:name="com.example.CISC4900.WeightliftingApp.NavigationDrawer">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.CISC4900.WeightliftingApp.LogFragment"
            android:launchMode="singleTop" />
        <activity
            android:name="com.example.CISC4900.WeightliftingApp.AddEditExerciseActivity"
            android:parentActivityName="com.example.CISC4900.WeightliftingApp.LogFragment" />
        <activity android:name="com.example.CISC4900.WeightliftingApp.OneRepMaxFragment" />
    </application>

</manifest>